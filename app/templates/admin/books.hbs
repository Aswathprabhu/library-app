<h2 style="color:#ffffff">List of Available Books</h2>

{{#each model as |library|}}
<div class="col-md-4">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">{{library.name}}</h3>
    </div>
    <div class="panel-body">
      <p>Released Year: {{library.releasedyear}}</p>
      <p>Author: {{library.author}}</p>
      <p>No of Books:{{library.noofbooks}}</p>
    </div>
    <div class="panel-footer text-right">
      <br>
    </div>
  </div>
</div>
{{/each}}
